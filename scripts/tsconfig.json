{
    "compilerOptions": {
      "target": "ES2020",
      "lib": [
        "dom",
        "dom.iterable",
        "ES2020"
      ],
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "CommonJS",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
    },
    "setupFiles": [
      "core-js"
    ],
    "include": [
      "./"
    ]
  }